package com.epam.hw2_2;

public class hw2_2 {

    public static void main(String[] args) {
    
        double deposit = 1000;
        int days = 181;
        int percents = 12;
        
        double income = (deposit * (percents/100) * days) / 365 * 100;
        System.out.println(income);
    }
}
